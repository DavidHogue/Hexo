#header
  padding 20px 0
  background-color #800
  color #ff9000
  border-top 5px solid black

  a
    color #ff9000
    
  h1
    font-weight normal
    font-size 2.5em
    line-height 1
    display inline

  h2
    font-weight normal
    font-size 1.2em
    font-family font-default
    line-height 1
    margin-top 10px
    margin-left 20px
    display inline
  
  #main-nav
    font-family font-default
    line-height 2.5em
    position relative
    top 45px
    ul
      list-style none
      li
        float left
        margin-left 30px